<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:Wpf_BinaryPhaseDiagram.XamlResources">
    <!--这里主要存放界面显示用的文字，试图在多语言之间进行切换-->
    <sys:String x:Key="MainWindowTitleString">二元相图软件</sys:String>
    <sys:String x:Key="SearchPanelTitleString">搜索面板</sys:String>
    <sys:String x:Key="ElementAString">元素A</sys:String>
    <sys:String x:Key="ElementBString">元素B</sys:String>
    <sys:String x:Key="SearchButtonString">立即搜索!</sys:String>
